{
  "name": "ctfnote-api",
  "version": "1.0.0",
  "description": "GraphQL API for CTFNotes",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "db-migrate -e prod up && node dist/index.js",
    "build": "tsc",
    "lint": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/*.ts'",
    "dev": "NODE_ENV=development nodemon src/index.ts"
  },
  "lint-staged": {
    "src/**/*.{ts,css,html,vue}": [
      "yarn run prettier --write"
    ]
  },
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.0",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-pubsub": "^4.11.0",
    "axios": "^0.21.1",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphile-migrate": "^1.0.2",
    "graphile-utils": "^4.11.2",
    "postgraphile": "^4.11.0",
    "postgraphile-plugin-connection-filter": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}