{
  "name": "ctfnote-api",
  "version": "1.0.0",
  "description": "GraphQL API for CTFNotes",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "lint": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "dev": "DEBUG=postgraphile:postgres:error NODE_ENV=development nodemon src/index.ts",
    "dev:migrate": "DATABASE_URL= yarn run db-migrate -e dev up"
  },
  "lint-staged": {
    "src/**/*.{ts,css,html,vue}": [
      "yarn run prettier --write"
    ]
  },
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.2",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/operation-hooks": "^1.0.0",
    "@graphile/pg-pubsub": "^4.13.0",
    "axios": "^1.3.4",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphile-utils": "^4.13.0",
    "graphql": "^16.6.0",
    "graphql-upload-ts": "^2.0.5",
    "ical-generator": "^3.2.1",
    "postgraphile": "^4.13.0",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "postgres-migrations": "^5.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
