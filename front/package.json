    "eslint": "^7.12.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-quasar": "^1.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.4"
