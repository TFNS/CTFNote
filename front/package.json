    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "babel-runtime": "^6.26.0",
    "color-hash": "^1.0.3",
    "core-js": "^3.6.5",
    "quasar": "^1.0.0",
    "slugify": "^1.5.0",
    "socket.io": "^2.4.0",
